{
	"name": "codehacks",
	"displayName": "CodeHacks",
	"description": "Able to delete console.log statements, create a new console.log based on what you select, installing dependencies automatically based on what you require",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.runCode",
		"onCommand:extension.helloWorld",
		"onCommand:extension.deleteAllLogStatements",
		"onCommand:extension.addLogStatements",
		"onCommand:extension.installDependencies",
		"onCommand:extension.commentAllLogStatements",
		"onCommand:extension.uncommentAllLogStatements",
		"onCommand:extension.makeComponent",
		"onCommand:extension.runEnv"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.runEnv",
				"title": "Run Env"
			},
			{
				"command": "extension.deleteAllLogStatements",
				"title": "Delete Log"
			},
			{
				"command": "extension.addLogStatements",
				"title": "Console Logger"
			},
			{
				"command": "extension.installDependencies",
				"title": "Install Dependencies"
			},
			{
				"command": "extension.runCode",
				"title": "Run Code Hacks"
			},
			{
				"command": "extension.commentAllLogStatements",
				"title": "Comment Log"
			},
			{
				"command": "extension.uncommentAllLogStatements",
				"title": "Uncomment Log"
			},
			{
				"command": "extension.makeComponent",
				"title": "Make Component"
			}
		],
		"keybindings": [
			{
				"command": "extension.deleteAllLogStatements",
				"mac": "shift+cmd+i",
				"key": "ctrl+alt+i"
			},
			{
				"command": "extension.commentAllLogStatements",
				"mac": "shift+cmd+0",
				"key": "ctrl+alt+0"
			},
			{
				"command": "extension.uncommentAllLogStatements",
				"mac": "shift+cmd+9",
				"key": "ctrl+alt+9"
			},
			{
				"command": "extension.installDependencies",
				"mac": "shift+cmd+8",
				"key": "ctrl+alt+8"
			},
			{
				"command": "extension.runCode",
				"mac": "shift+cmd+7",
				"key": "ctrl+alt+7"
			},
			{
				"command": "extension.addLogStatements",
				"mac": "shift+cmd+a",
				"key": "ctrl+alt+a"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"child_process": "^1.0.2",
		"path": "^0.12.7"
	}
}
